{
	"lsp": {
		"biome": {
			"enable_lsp_tasks": false
		},
		"deno": {
			"settings": {
				"deno": {
					"enable": true
				}
			}
		}
	},
	"languages": {
		"JavaScript": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint",
				"!biome"
			],
			"formatter": "language_server"
		},
		"TypeScript": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint",
				"!biome"
			],
			"formatter": "language_server"
		},
		"TSX": {
			"language_servers": [
				"deno",
				"!typescript-language-server",
				"!vtsls",
				"!eslint",
				"!biome"
			],
			"formatter": "language_server"
		}
	}
}
